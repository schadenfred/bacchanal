- unless @winery.nil?
  = meta_tags(title: @winery.name, author: @winery.try(:owner), description: @winery.try(:description), keywords: @winery.try(:keywords), url: request.original_url, og: { title: @winery.name, type: { winery: { varietal: @winery.try(:varietals), appellations: @winery.try(:wines_appellations).map(&:name) } }, image: @winery.try(:default_gallery).try(:photos).try(:first).try(:image).try(:url) }, twitter: { card: "summary"}  )